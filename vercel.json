{
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "dist"
            }
        }
    ],
    "routes": [
        {
            "src": "/voice-community",
            "dest": "/voice-community.html"
        },
        {
            "src": "/static/(.*)",
            "headers": { "cache-control": "public, max-age=31536000, immutable" },
            "dest": "/static/$1"
        },
        { "handle": "filesystem" },
        { "src": "/(.*)", "dest": "/index.html" }
    ]
}
  