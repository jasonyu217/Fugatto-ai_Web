{
    "builds": [
      {
        "src": "requirements.txt",
        "use": "@vercel/python",
        "config": {
          "runtime": "vercel-python@3.9",
          "installCommand": "pip install --upgrade pip setuptools wheel && pip install -r requirements.txt"
        }
      },
      {
        "src": "*.py",
        "use": "@vercel/python"
      }
    ]
  }
  